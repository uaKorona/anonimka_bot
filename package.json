{
  "name": "anonimka_bot",
  "version": "1.0.1",
  "description": "Telegram bot for anonymous message forwarding",
  "main": "handler.js",
  "scripts": {
    "serverless": "serverless",
    "release": "serverless deploy",
    "purge": "serverless remove",
    "set-webhook": "telegraf -m setWebhook",

    "watch-ts": "tsc -w",
    "serve-debug": "nodemon --inspect dist/handler.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uaKorona/anonimka_bot.git"
  },
  "author": "Dorian Gray",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uaKorona/anonimka_bot/issues"
  },
  "homepage": "https://github.com/uaKorona/anonimka_bot#readme",
  "dependencies": {
    "telegraf": "^4.9.0",
    "serverless-http": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.29",
    "serverless": "^3.22.0",
    "serverless-plugin-typescript": "^2.1.2",
    "typescript": "^4.7.4",
    "ts-node": "^10.7.0",
    "concurrently": "^7.2.1",
    "nodemon": "^2.0.16"
  },
  "engines": {
    "node": "16.13",
    "npm": "8.1"
  }
}
