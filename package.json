{
  "name": "anonimka_bot",
  "version": "1.0.0",
  "description": "Telegram bot for anonymous message forwarding",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uaKorona/anonimka_bot.git"
  },
  "author": "Dorian Gray",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uaKorona/anonimka_bot/issues"
  },
  "homepage": "https://github.com/uaKorona/anonimka_bot#readme",
  "dependencies": {
    "telegraf": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "concurrently": "^7.1.0",
    "eslint": "^8.14.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  },
  "engines": {
    "node": "16.13.",
    "npm": "8.1"
  }
}
